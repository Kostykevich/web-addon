{
  "manifest_version": 3,
  "name": "Assistant HELPER",
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "*://127.0.0.1/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.google.com/*", "*://kinogo.online/*"],
      "js": ["content.js"]
    }
  ]
}